  "scripts": {
    "start": "lessc public/css/bundle.less > public/css/bundle.css & watchify -o public/js/bundle.js -v -d app/frontend/index.js & NODE_PATH=./config:./app/controllers NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "test": "jest & NODE_PATH=./config:./app/controllers NODE_ENV=test ./node_modules/.bin/mocha --reporter spec --timeout 10s00 app/tests/test-*.js",
    "build": "echo run $ulimit -n 2560 && lessc public/css/bundle.less -x > public/css/bundle.min.css & browserify app/frontend/index.js -t [envify --NODE_ENV production] | uglifyjs -cm > public/js/bundle.min.js"
    },
